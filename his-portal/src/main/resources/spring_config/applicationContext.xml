	<context:component-scan base-package="cn.honry.patient.*" />
	<context:component-scan base-package="cn.honry.emrs.*" />
	<context:component-scan base-package="cn.honry.technical.*" />
	<context:component-scan base-package="cn.honry.inner.*" />
	<context:component-scan base-package="cn.honry.sys.*" />
	<context:component-scan base-package="cn.honry.patinent.*" />
	<context:component-scan base-package="cn.honry.business.*" />
	<context:component-scan base-package="cn.honry.register.*" />
	<context:component-scan base-package="cn.honry.outpatient.*" />
	<context:component-scan base-package="cn.honry.inpatient.*" />
