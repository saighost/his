	<context:component-scan base-package="cn.honry.report.*" />
	<context:component-scan base-package="cn.honry.except.*" />
	<context:component-scan base-package="cn.honry.search.*" />
	<context:component-scan base-package="cn.honry.hiasMongo.*" />
	<context:component-scan base-package="cn.honry.oa.*" />
