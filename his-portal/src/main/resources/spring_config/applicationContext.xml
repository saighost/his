<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
         http://www.springframework.org/schema/context 
         http://www.springframework.org/schema/context/spring-context-4.2.xsd">

	<!-- spring管理properties文件 -->
	<context:property-placeholder location="classpath:elasticsearch.properties" ignore-unresolvable="true"/>
	<context:property-placeholder location="classpath:rabbitmq.properties" ignore-unresolvable="true"/>

	<!-- <context:annotation-config/> -->
	<!-- component-scan自动搜索@Component , @Controller , @Service , @Repository等标注的类 -->
