	t.OUT_NUM ,
	t.PURCHASE_PRICE,
	t.PURCHASE_COST,
	t.RETAIL_PRICE,
	t.RETAIL_COST 
FROM
	T_DRUG_OUTSTORE_NOW T
LEFT JOIN T_DRUG_INFO tdi ON T.DRUG_CODE = tdi.DRUG_CODE
LEFT JOIN T_BUSINESS_DICTIONARY tcu ON tdi.DRUG_USEMODE = tcu.CODE_ENCODE AND tcu.CODE_TYPE='useage'
LEFT JOIN T_BUSINESS_DICTIONARY tcu1 ON T.PACK_UNIT = tcu1.CODE_ENCODE AND tcu.CODE_TYPE='packunit'
