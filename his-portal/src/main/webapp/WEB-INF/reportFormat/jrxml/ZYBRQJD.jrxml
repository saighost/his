	<queryString><![CDATA[select i.NURSE_CELL_NAME as DEPTNAME, --病区
       i.bed_name as BEDNAME, --病床
       i.patient_name as NAME, --患者姓名
       l.remark as REMARK, --备注
       to_char(l.leave_date, 'yyyy') as LEAVEYYYY, --请假时间年
