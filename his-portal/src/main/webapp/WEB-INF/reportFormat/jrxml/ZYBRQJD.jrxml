       to_char(l.leave_date, 'mm') as LEAVEMMMM, --请假时间分
       to_char(l.leave_date + l.leave_days, 'yyyy') as REMARKYYYY, --预约时间年
       to_char(l.leave_date + l.leave_days, 'MM') as REMARKMM, --预约时间月
       to_char(l.leave_date + l.leave_days, 'dd') as REMARKDD, --预约时间日
       to_char(l.leave_date + l.leave_days, 'HH') as REMARKHH, --预约时间时
       to_char(l.leave_date + l.leave_days, 'mm') as REMARKMMMM, --预约时间分
       p.patient_phone as PHONE, --联系电话
       i.medicalrecord_id as NO --病例号
  from t_inpatient_nui_leave l
  left join t_inpatient_info_now i on i.inpatient_no = l.inpatient_no
  left join t_patient p on p.medicalrecord_id = i.medicalrecord_id
 where l.id = $P{id2}]]></queryString>

