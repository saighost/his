	<queryString><![CDATA[select i.DRUG_BASICINPUTCODE as drugBasicCode, i.DRUG_BIDDINGCODE as drugBiddingCode,
 i.DRUG_ID AS drugId, i.DRUG_NAME AS drugName, 
 DECODE (o.OUT_STATE,'0','申请', '1', '审批', '2', '核准') AS outState, 
i.DRUG_SPEC AS drugSpec, ROUND((SUM(o.OUT_NUM) / i.DRUG_PACKAGINGNUM), 2) AS num,
 DECODE (o.out_kind,'1','门诊摆药','2','内部入库','3','门诊退药','4','住院摆药', '5', '住院退药') AS outKind,
 (select t.code_name from t_business_dictionary t where t.code_encode= i.DRUG_PACKAGINGUNIT and t.code_type = 'drugPackagingunit')  AS drugPackgingUnit,
round(o.retail_price, 2) * ROUND((SUM(o.OUT_NUM) / i.DRUG_PACKAGINGNUM), 2) AS sum, 
ROUND(o.retail_price, 2) as retailPrice  
from T_DRUG_INFO i,T_DRUG_OUTSTORE o WHERE o.DRUG_CODE = i.DRUG_ID  

GROUP BY i.DRUG_BASICINPUTCODE,i.DRUG_BIDDINGCODE, i.DRUG_ID,i.DRUG_NAME,
 o.OUT_STATE,o.out_kind, DECODE(o.OUT_STATE,'0','申请','1','审批', '2','核准'),
DECODE(o.out_kind, '1', '门诊摆药', '2', '内部入库','3','门诊退药', '4', '住院摆药', '5','住院退药'),
 i.DRUG_SPEC,i.DRUG_PACKAGINGNUM,i.DRUG_PACKAGINGUNIT ,o.retail_price]]></queryString>

