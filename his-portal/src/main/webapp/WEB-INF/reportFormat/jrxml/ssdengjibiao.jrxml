	<parameter name="opId" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select t.name,
        decode(t.PASOURCE,'1',(select PAYKIND_NAME as pay from t_Register_Main_Now n where n.clinic_code=t.clinic_code),'2',(select di.code_name as pay from t_inpatient_info_now left join t_business_dictionary  di on  di.CODE_ENCODE= PAYKIND_CODE where INPATIENT_NO=t.clinic_code and di.code_type='paykind'),'未知') as pay,
      (select b.code_name from t_Business_Dictionary b where b.code_encode=t.CONSOLE_TYPE and b.code_type='tabletype') as console,
       (select b.code_name from t_Business_Dictionary b where b.code_encode=t.OPS_KIND and b.code_type='operatetype')as opskind,
       t.OPERATIONDATE,
       (select d.dept_name from t_Department d where d.dept_code= t.EXEC_DEPT) as exec,
       t.clinic_code,
       decode(t.sex_code,'1','男'，'2','女','未知') as sex,
       a.age || a.age_unit as age,
       (select dept_name from t_Department where dept_code= t.dept_code) as dept,
       (select wm_concat(diag_name ) from t_operation_diagnose where operation_id = $P{opId} and del_flg=0  ) as diag,
       (select wm_concat(item_name)from t_operation_item where  operation_id = $P{opId} and del_flg=0)  as item,
       (select b.code_name from t_Business_Dictionary b where b.code_encode=a.ANE_WAY and  b.code_type='aneWay') as aneway,
       (select m.employee_name from t_employee m where m.employee_jobno= t.OPS_DOCD ) as opsDocd,
       (select empl_name from t_operation_arrange where fore_flag='3' and ROLE_CODE ='thelper0' and operation_id=$P{opId} and del_flg=0 and stop_flg=0) as thel1,
       (select empl_name from t_operation_arrange where fore_flag='3' and ROLE_CODE ='thelper1' and operation_id=$P{opId}  and del_flg=0 and stop_flg=0) as thel2,
       (select empl_name from t_operation_arrange where fore_flag='3' and ROLE_CODE ='thelper2' and operation_id=$P{opId}  and del_flg=0 and stop_flg=0) as thel3,
      (select wm_concat( empl_name) from t_operation_arrange where fore_flag='3' and ROLE_CODE like '%wash%' and operation_id=$P{opId}  and del_flg=0 and stop_flg=0) as wash,
       (select  wm_concat(empl_name) from t_operation_arrange where fore_flag='3' and ROLE_CODE like '%tour%' and operation_id=$P{opId}  and del_flg=0 and stop_flg=0 ) as tour,
        (select m.employee_name from t_employee m where m.employee_jobno=t.APPLY_DOCD) apply,
        t.REMARK,
       a.BED_NO bedName
  from t_Operation_Record t
  left join t_Operation_Apply a on t.operation_id = a.op_id
 where t.del_flg = 0
   and t.stop_flg = 0
 and t.operation_id=$P{opId}]]></queryString>

