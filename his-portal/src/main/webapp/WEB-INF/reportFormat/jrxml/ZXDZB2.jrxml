c.frequency_name as pinci,--频次
c.mo_note2 as note,--备注
c.inpatient_no as ip,--住院流水号
c.specs as specs,--规格 
(select ttt.code_name from t_code_useage ttt where ttt.code_id= c.usage_code) as yongfa,--用法
c.dose_once as jiliang--剂量
from t_inpatient_execdrug c where c.del_flg=0 and c.stop_flg=0 and  (c.type_code,c.usage_code,c.drug_type) in (select b.type_code,b.usage_code,b.drug_type from t_inpatient_drugbilldetail b where b.del_flg=0 and b.stop_flg=0 and (b.bill_no)
in (select a.bill_no from t_inpatient_execbill a where a.bill_no=$P{billNo} and a.del_flg=0 and a.stop_flg=0))
and c.inpatient_no=$P{inpatientNo}]]></queryString>

