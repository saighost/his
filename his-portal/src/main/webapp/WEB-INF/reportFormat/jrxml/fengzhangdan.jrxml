	<queryString><![CDATA[SELECT
	T .TRADE_NAME,
	td.dept_name,
	T .SPECS,
	tcd.code_Name,
	T .BATCH_NO,
	T .VALID_DATE,
	t.FSTORE_NUM,
tdm.MANUFACTURER_NAME,
	(CASE WHEN PROFIT_FLAG=0 THEN '盘亏'
	 WHEN PROFIT_FLAG=1 THEN '盘盈' END) flag,
	t.PROFIT_LOSS_NUM,
	t.PURCHASE_PRICE,
	t.RETAIL_PRICE	
FROM
	T_DRUG_CHECKDETAIL T
LEFT JOIN T_DEPARTMENT td ON T .DRUG_DEPT_CODE = td.DEPT_ID
LEFT JOIN t_business_dictionary tcd ON T.PACK_UNIT = tcd.code_encode and  tcd.code_type = 'packunit'
LEFT JOIN T_DRUG_MANUFACTURER tdm ON t.PRODUCER = tdm.MANUFACTURER_ID
WHERE
	t.CHECK_CODE = $P{cdid}
	AND t.DRUG_DEPT_CODE = $P{deptCode} 
	AND t.CHECK_STATE = 0]]></queryString>

