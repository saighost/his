t.PURCHASE_PRICE,t.RETAIL_PRICE
from t_drug_outstore t
Left join t_department d on t.drug_dept_code=d.dept_id
Left join t_code_drugtype c on t.DRUG_TYPE=c.code_id


where t.OUT_STATE=2 and t.RETURN_NUM>0 and t.ID in (with temp as (select $P{ID} text
                                from dual)
  select substr(text,
          instr(text, ',', 1, rn) + 1,
          instr(text, ',', 1, rn + 1) - instr(text, ',', 1, rn) - 1) text
    from (select ',' || t1.text || ',' text, t2.rn
      from temp t1,
           (select rownum rn
              from all_objects
             where rownum <= (select length(text) -
                                     length(replace(text, ',', '')) + 1
                                from temp)) t2))]]></queryString>

