	<queryString><![CDATA[select p.patient_name  as name,--姓名
       decode( p.patient_sex ,'1','男'，'2','女','3','未知'  ) as sex,--患者性别
       c.cnsl_note     as zhaiyao,--会诊摘要
       to_char( c.cnsl_date,'yyyy' ) as condateYYYY,
       to_char( c.cnsl_date,'MM' ) as condateMM,
       to_char( c.cnsl_date,'dd' ) as condatedd, 
       d1.dept_name as hzdept,--会诊科室
	c.location as address,--会诊地点
       e1.employee_name as hzdoc,--会诊医生
       to_char( c.mo_stdt,'yyyy' ) as sqdateYYYY,
       to_char( c.mo_stdt,'MM' ) as sqdateMM,
       to_char( c.mo_stdt,'dd' ) as sqdatedd, 

       to_char( c.mo_eddt,'yyyy' ) as jzdateYYYY,
       to_char( c.mo_eddt,'MM' ) as jzdateMM,
       to_char( c.cnsl_date,'dd' ) as jzdatedd,
       c.location as address，--会诊地点
       e2.employee_name as sqdoc,--申请医生
       d2.dept_name as sqdept,--申请科室
       to_char( c.apply_date,'yyyy' ) as applydateYYYY,
       to_char( c.apply_date,'MM' ) as applydateMM,
       to_char( c.apply_date,'dd' ) as applydatedd,
       decode(c.urgent_flag,'0','','1','√' )as juflag,--加急标志(急)
       decode(c.urgent_flag,'0','√','1','' )as puflag,--加急标志(普通)
       decode(c.CNSL_KIND,'0','','1','','2','√' )as ckind,--院外会诊
       decode(c.CREATE_ORDER_FLAG,'0','','1','√' ) as kaili,--能否开立遗嘱
       c.CNSL_NOTE3 as suggest --会诊意见
  from t_inpatient_consultation c
  left join t_patient p on p.medicalrecord_id = c.patient_no
  left join T_REGISTER_INFO r on r.MEDICALRECORD_ID = c.inpatient_no
  left join t_department d1 on d1.dept_id = c.cnsl_deptcd
  left join t_department d2 on d2.dept_id =c.dept_code
  left join t_employee e1 on e1.employee_jobno = c.cnsl_doccd
  left join t_employee e2 on e2.employee_jobno=c.doc_code
  where c.cnsl_id=$P{mid}]]></queryString>

