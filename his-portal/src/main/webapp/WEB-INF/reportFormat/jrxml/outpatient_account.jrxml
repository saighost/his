	<queryString><![CDATA[--根据就诊卡号 查询患者就诊卡编号
--根据就诊卡编号  查询患者门诊账户编号
--根据门诊账户编号  时间倒叙即可

select d.patient_name,  --患者姓名
       d.prepay_cost,  --预存金额
       MONEY2CHINESE(d.prepay_cost) as prepay_costdx,  --预存金大写  
       to_char(d.createtime, 'yyyy-mm-dd hh24: mi: ss') as createtime,  --收款时间
       to_char(sysdate, 'yyyy-mm-dd hh24: mi: ss') as s_date,  --打印时间
       d.receipt_no,  --预存金收据     
       nvl2(d.createuser,
            (select e.employee_name
               from t_employee e
              where e.Employee_Jobno = d.createuser),
            null) as createuser  --收款人
  from (select c.*
          from t_outpatient_outprepay c
         where c.account_id =
               (select b.account_id
                  from t_outpatient_account b
                 where b.idcard_id =
                       (select a.idcard_id
                          from t_patient_idcard a
                         where a.idcard_no = $P{tid}))
         order by c.updatetime desc) d
 where rownum = 1]]></queryString>

