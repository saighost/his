	<queryString><![CDATA[select c.undrug_name as dname, --治疗项目
       c.exec_prndate as ddate, --执行时间
       (select t.USER_NAME
          from t_sys_user t
         where t.user_account = c.exec_prnusercd) as ename, --执行人
       c.dfq_cexp as pinci, --频次
       c.mo_note2 as note, --备注
       c.inpatient_no as ip, --住院流水号
       c.Qty_Tot as specs, --数量
       c.UNIT_PRICE as yongfa, --单价
       '10.0000' as jiliang --剂量
  from t_inpatient_execundrug_now c
 where c.del_flg = 0
   and c.stop_flg = 0
   and (c.type_code, c.undrug_code, c.class_code) in
       (select b.type_code, b.usage_code, b.drug_type
          from t_inpatient_drugbilldetail b
         where b.del_flg = 0
           and b.stop_flg = 0
           and (b.bill_no) in (select a.bill_no
                                 from t_inpatient_execbill a
                                where a.bill_no = $P{billNo}
                                  and a.del_flg = 0
                                  and a.stop_flg = 0))
   and c.inpatient_no = $P{inpatientNo}]]></queryString>

