<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.honry.base.bean.model">
    <class name="FinanceInvoicedetail" table="T_FINANCE_INVOICEDETAIL">
        <id name="id" type="java.lang.String">
            <column name="INVOICE_ID" />
            <generator class="uuid" />
        </id>
        <property name="invoiceNo" type="java.lang.String">
            <column name="INVOICE_NO"  />
        </property>
        <property name="transType" type="java.lang.Integer">
            <column name="TRANS_TYPE" />
        </property>
        <property name="invoSequence" type="java.lang.String">
            <column name="INVO_SEQUENCE"  />
        </property>
        <property name="invoCode" type="java.lang.String">
            <column name="INVO_CODE"  />
        </property>
        <property name="invoName" type="java.lang.String">
            <column name="INVO_NAME" />
        </property>
        <property name="pubCost" type="java.lang.Double">
            <column name="PUB_COST" precision="8" />
        </property>
        <property name="ownCost" type="java.lang.Double">
            <column name="OWN_COST" precision="8" />
        </property>
        <property name="payCost" type="java.lang.Double">
            <column name="PAY_COST" precision="8" />
        </property>
        <property name="deptCode" type="java.lang.String">
            <column name="DEPT_CODE" />
        </property>
        <property name="deptName" type="java.lang.String">
            <column name="DEPT_NAME"  />
        </property>
        <property name="operDate" type="java.util.Date">
            <column name="OPER_DATE" />
        </property>
        <property name="operCode" type="java.lang.String">
            <column name="OPER_CODE" />
        </property>
        <property name="balanceFlag" type="java.lang.Integer">
            <column name="BALANCE_FLAG"  />
        </property>
        <property name="balanceNo" type="java.lang.String">
            <column name="BALANCE_NO" />
        </property>
        <property name="balanceOpcd" type="java.lang.String">
            <column name="BALANCE_OPCD" />
        </property>
        <property name="balanceDate" type="java.util.Date">
            <column name="BALANCE_DATE"/>
        </property>
        <property name="cancelFlag" type="java.lang.Integer">
            <column name="CANCEL_FLAG"  />
        </property>
        <property name="invoiceSeq" type="java.lang.String">
            <column name="INVOICE_SEQ" />
        </property>
       <property name="createUser" type="java.lang.String">
           <column name="CREATEUSER" />
        </property>
        <property name="createDept" type="java.lang.String">
            <column name="CREATEDEPT" />
        </property>        
        <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATETIME" length="19" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="UPDATEUSER" />
        </property>      
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="UPDATETIME" length="19" />
        </property>
        <property name="deleteUser" type="java.lang.String">
            <column name="DELETEUSER" />
        </property>      
        <property name="deleteTime" type="java.sql.Timestamp">
            <column name="DELETETIME" length="19" />
        </property>        
        <property name="del_flg" type="java.lang.Integer">
            <column name="DEL_FLG" />
        </property>
        <property name="stop_flg" type="java.lang.Integer">
            <column name="STOP_FLG" />
        </property>
        <property name="operName" type="java.lang.String">
            <column name="OPER_NAME" length="50" />
        </property> 
        <property name="balanceOpceName" type="java.lang.String">
            <column name="BALANCE_OPCD_NAME" length="50" />
        </property> 
    </class>
</hibernate-mapping>
