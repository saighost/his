<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.honry.base.bean.model">
	<class name="BusinessChange" table="T_BUSINESS_CHANGE">
		<id name="id" type="java.lang.String">
			<column name="ID" />
		</id>
		<property name="subjectName" type="java.lang.String">
			<column name="SUBJECT_NAME" />
		</property>
		<property name="subjectId" type="java.lang.String">
			<column name="SUBJECT_ID" />
		</property>
		<property name="tableName" type="java.lang.String">
			<column name="TABLE_NAEM" />
		</property>
		
		<property name="column" type="java.lang.String">
			<column name="COLUMN" />
		</property>
		<property name="columnName" type="java.lang.String">
			<column name="COLUMN_NAME" />
		</property>
		<property name="oldValue" type="java.lang.String">
			<column name="OLD_VALUE" />
		</property>
		<property name="newValue" type="java.lang.String">
			<column name="NEW_VALUE" />
		</property>
		<property name="SessionId" type="java.lang.String">
			<column name="SESSIONID" >
				<comment>会话ID </comment>
			</column>
		</property>
		<property name="IP" type="java.lang.String">
			<column name="IP" />
		</property>
		<property name="createUser" type="java.lang.String">
			<column name="CREATEUSER" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATETIME"/>
		</property>
		<property name="createDept" type="java.lang.String">
			<column name="CREATEDEPT" />
		</property>
		
	</class>

</hibernate-mapping>
