<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.honry.base.bean.model">
	<class name="Schedule" table="M_SCHEDULE">
			 <id name="id" type="java.lang.String">
	            <column name="SCHEDULE_CODES" />
	            <generator class="uuid" />
	        </id>	        
	         <property name="title" type="java.lang.String">
                <column name="SCHEDULE_TITILE" />
            </property>
	         <property name="dayFlg" type="java.lang.Integer">
                <column name="ALL_DAY_FLG" length="1" />
            </property>
            <property name="start" type="java.sql.Timestamp">
                <column name="SCHEDULE_START_TIME"  />
            </property>
            <property name="end" type="java.sql.Timestamp">
                <column name="SCHEDULE_END_TIME" />
            </property>
            <property name="timeMinus" type="java.lang.Integer">
                <column name="SCHEDULE_TIME_MINUS" />
            </property>
            <property name="timeUnit" type="java.lang.String">
                <column name="SCHEDULE_TIME_UNIT" />
            </property>
            <property name="time" type="java.sql.Timestamp">
                <column name="SCHEDULE_TIME" />
            </property>
            <property name="remark" type="java.lang.String">
                <column name="SCHEDULE_REMARK" />
            </property>
            <property name="userId" type="java.lang.String">
                <column name="USER_ACCOUNT" />
            </property>
            <property name="createUser" type="java.lang.String">
            <column name="CREATEUSER" />
	        </property>
	        <property name="createDept" type="java.lang.String">
	            <column name="CREATEDEPT" />
	        </property>        
	        <property name="createTime" type="java.sql.Timestamp">
	            <column name="CREATETIME" />
	        </property>
	        <property name="updateUser" type="java.lang.String">
	            <column name="UPDATEUSER" />
	        </property>      
	        <property name="updateTime" type="java.sql.Timestamp">
	            <column name="UPDATETIME"/>
	        </property>
	        <property name="deleteUser" type="java.lang.String">
	            <column name="DELETEUSER" />
	        </property>      
	        <property name="deleteTime" type="java.sql.Timestamp">
	            <column name="DELETETIME" />
	        </property>        
	        <property name="del_flg" type="java.lang.Integer">
	            <column name="DEL_FLG" />
	        </property>
	        <property name="stop_flg" type="java.lang.Integer">
	            <column name="STOP_FLG" />
	        </property>
	        <property name="hhmm" type="java.lang.String">
	            <column name="TIME_MINUS_HH_MM" />
	        </property>    
	        <property name="isZDY" type="java.lang.String">
	            <column name="IS_ZDY" />
	        </property>    
	        <property name="scheduleFlg" type="java.lang.String">
	            <column name="SCHEDULE_FLG" />
	        </property>    
	        <property name="isFinish" type="java.lang.Integer">
	            <column name="IS_FINISH" />
	        </property>    
        </class>
</hibernate-mapping>
