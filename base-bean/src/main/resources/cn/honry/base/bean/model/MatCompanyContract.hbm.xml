<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.honry.base.bean.model">
        <class name="MatCompanyContract" table="T_MAT_COMPANY_CONTRACT">
	        <id name="id" type="java.lang.String">
	            <column name="ID" />
	            <generator class="uuid" />
	        </id>
            <property name="icompanyId" type="java.lang.String">
                <column name="COMPANY_ID" />
            </property>
            <property name="companyCode" type="java.lang.String">
                <column name="COMPANY_CODE" />
            </property>
            <property name="contractType" type="java.lang.Integer">
                <column name="CONTRACT_TYPE" />
            </property>
            <property name="contractFilename" type="java.lang.String">
                <column name="CONTRACT_FILENAME" />
            </property>
            <property name="contractFilepath" type="java.lang.String">
                <column name="CONTRACT_FILEPATH"  />
            </property>
            <property name="contractCode" type="java.lang.String">
                <column name="CONTRACT_CODE" />
            </property>
            <property name="contractDate" type="java.util.Date">
                <column name="CONTRACT_DATE" />
            </property>
            <property name="overDate" type="java.util.Date">
                <column name="OVER_DATE" />
            </property>
            <property name="validFlag" type="java.lang.Integer">
                <column name="VALID_FLAG" />
            </property>
            <property name="mark" type="java.lang.String">
                <column name="MARK"/>
            </property>
            <property name="operCode" type="java.lang.String">
                <column name="OPER_CODE" />
            </property>
            <property name="operDate" type="java.util.Date">
                <column name="OPER_DATE" />
            </property>
            </class>
</hibernate-mapping>