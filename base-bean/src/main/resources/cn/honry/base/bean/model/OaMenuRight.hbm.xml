<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.honry.base.bean.model">
    <class name="OaMenuRight" table="T_OA_MENURIGHT" >
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="rightType" type="java.lang.String">
            <column name="RIGHT_RIGHTTYPE" >
                <comment>权限类型</comment>
            </column>
        </property>
        <property name="right" type="java.lang.String">
            <column name="RIGHT_TYPE" >
                <comment>选择类型</comment>
            </column>
        </property>
        <property name="code" type="java.lang.String">
            <column name="RIGHT_CODE" >
                <comment>类型code</comment>
            </column>
        </property>
        <property name="menuId" type="java.lang.String">
            <column name="MENU_ID" >
                <comment>栏目id</comment>
            </column>
        </property>
        <property name="codeName" type="java.lang.String">
            <column name="CODENAME" >
                <comment>栏目id</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
