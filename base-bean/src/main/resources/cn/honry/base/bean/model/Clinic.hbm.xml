<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="cn.honry.base.bean.model">
    <class name="Clinic" table="T_CLINIC">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="clinicName" type="java.lang.String">
            <column name="CLINIC_NAME" />
        </property>
        <property name="clinicPiyin" type="java.lang.String">
            <column name="CLINIC_PIYIN" />
        </property>
        <property name="clinicWb" type="java.lang.String">
            <column name="CLINIC_WB" />
        </property>
        <property name="clinicInputcode" type="java.lang.String">
            <column name="CLINIC_INPUTCODE" />
        </property>
        <property name="clinicDeptId" type="java.lang.String">
            <column name="CLINIC_DEPT_ID" />
        </property>
        <property name="clinicRemark" type="java.lang.String">
            <column name="CLINIC_REMARK" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="CREATEUSER" />
        </property>
        <property name="createDept" type="java.lang.String">
            <column name="CREATEDEPT"  />
        </property>        
        <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATETIME" length="19" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="UPDATEUSER" />
        </property>      
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="UPDATETIME" length="19" />
        </property>
        <property name="deleteUser" type="java.lang.String">
            <column name="DELETEUSER" />
        </property>      
        <property name="deleteTime" type="java.sql.Timestamp">
            <column name="DELETETIME" length="19" />
        </property>        
        <property name="del_flg" type="java.lang.Integer">
            <column name="DEL_FLG" />
        </property>
        <property name="stop_flg" type="java.lang.Integer">
            <column name="STOP_FLG" />
        </property>
    </class>
</hibernate-mapping>
