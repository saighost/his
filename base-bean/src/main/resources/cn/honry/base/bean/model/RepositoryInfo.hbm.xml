<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="cn.honry.base.bean.model">
    <class name="RepositoryInfo" table="T_REPOSITORY_INFO">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <property name="content" type="java.lang.String">
        	<column name="CONTENT"></column>
        </property>
        <property name="keyWord" type="java.lang.String">
        	<column name="KEYWORD"></column>
        </property>
        <property name="categCode" type="java.lang.String">
        	<column name="CATEG_CODE"></column>
        </property>
        <property name="categName" type="java.lang.String">
        	<column name="CATEG_NAME"></column>
        </property>
        <property name="origin" type="java.lang.String">
        	<column name="ORIGIN"></column>
        </property>
        <property name="attach" type="java.lang.String">
        	<column name="ATTACH"></column>
        </property>
        <property name="remark" type="java.lang.String">
        	<column name="REMARK"></column>
        </property>
        <property name="isOvert" type="java.lang.Integer">
        	<column name="ISOVERT"></column>
        </property>
        <property name="isCollect" type="java.lang.Integer">
        	<column name="ISCOLLECT"></column>
        </property>
        <property name="attachName" type="java.lang.String">
        	<column name="ATTACHNAME"></column>
        </property>
        <property name="pubFlg" type="java.lang.Integer">
        	<column name="PUBFLG"></column>
        </property>
        <property name="views" type="java.lang.Integer">
        	<column name="VIEWS"></column>
        </property>
        
        
        
        <property name="createUser" type="java.lang.String">
            <column name="CREATEUSER" />
        </property>
        <property name="createDept" type="java.lang.String">
            <column name="CREATEDEPT"  />
        </property>        
        <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATETIME" length="19" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="UPDATEUSER" />
        </property>      
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="UPDATETIME" length="19" />
        </property>
        <property name="deleteUser" type="java.lang.String">
            <column name="DELETEUSER" />
        </property>      
        <property name="deleteTime" type="java.sql.Timestamp">
            <column name="DELETETIME" length="19" />
        </property>        
        <property name="del_flg" type="java.lang.Integer">
            <column name="DEL_FLG" />
        </property>
        <property name="stop_flg" type="java.lang.Integer">
            <column name="STOP_FLG" />
        </property>
    </class>
</hibernate-mapping>
